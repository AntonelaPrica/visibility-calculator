{
	"npmScope": "ro-ubb",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"],
				"accessToken": "MmI1OWY3ZWUtYmJhNi00MzI5LWIwMDgtMzY4ZTcyNzkzOGQ3fHJlYWQtd3JpdGU="
			}
		}
	},
	"defaultProject": "visibility-calculator-api",
	"generators": {
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		}
	},
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
